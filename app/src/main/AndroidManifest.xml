<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidArchitectureExample"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ellaclub.androidarchitectureexample.simplest.mvc.MVCActivity" />
        <activity android:name="com.ellaclub.androidarchitectureexample.simplest.mvp.MVPActivity" />
        <activity android:name="com.ellaclub.androidarchitectureexample.simplest.mvvm.MVVMActivity" />
        <activity android:name="com.ellaclub.androidarchitectureexample.simplest.mvi.MVIActivity" />
        <activity android:name="com.ellaclub.androidarchitectureexample.simplest.compose.ComposeActivity" />
        <activity android:name=".network.compose.ComposeActivity" />
        <activity android:name=".network.mvc.MVCActivity" />
        <activity android:name=".network.mvp.MVPActivity" />
        <activity android:name=".network.mvvm.MVVMActivity" />
        <activity android:name=".network.mvi.MVIActivity" />
        <activity android:name=".feeds.compose.ComposeActivity" />
        <activity android:name=".feeds.mvc.MVCActivity" />
        <activity android:name=".feeds.mvp.MVPActivity" />
        <activity android:name=".feeds.mvvm.MVVMActivity" />
        <activity android:name=".feeds.mvi.MVIActivity" />
    </application>

</manifest>