<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/go_to_mvc"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Go to MVC example" />

    <Button
        android:id="@+id/go_to_mvp"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Go to MVP example" />

    <Button
        android:id="@+id/go_to_mvvm"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Go to MVVM example" />

    <Button
        android:id="@+id/go_to_mvi"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Go to MVI example" />

    <Button
        android:id="@+id/go_to_compose"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Go to JetPack Compose example" />

</LinearLayout>